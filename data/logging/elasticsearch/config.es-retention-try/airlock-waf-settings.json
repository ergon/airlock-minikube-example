{
  "index_patterns": [
    "airlock-waf-default-*",
    "airlock-logs-*"
  ],
  "aliases": {
    "airlock-waf-default": {
      "is_write_index": true
    }
  },
  "settings": {
    "index": {
      "lifecycle": {
        "name": "airlock-ilm-policy",
        "rollover_alias": "airlock-waf-default"
      },
      "number_of_replicas": "0",
      "number_of_shards": "1"
    }
  }
}