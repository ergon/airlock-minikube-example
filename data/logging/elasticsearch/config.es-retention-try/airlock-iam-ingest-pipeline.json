{
	"description": "Airlock IAM Default Ingest Pipeline",
	"processors": [
		{
			"set": {
				"if": "ctx.log_id == 'IAM-DETAIL'",
				"field": "_index",
				"value": "airlock-iam-detail-000001"
			}
		},
		{
			"set": {
				"if": "ctx.log_id == 'IAM-AUDIT'",
				"field": "_index",
				"value": "airlock-iam-audit-000001"
			}
		},
		{
			"set": {
				"if": "ctx.log_id == 'IAM-USERTRAIL'",
				"field": "_index",
				"value": "airlock-iam-usertrail-000001"
			}
		},
		{
			"set": {
				"if": "ctx.log_id == 'IAM-REPORTING'",
				"field": "_index",
				"value": "airlock-iam-reporting-000001"
			}
		}
	]
}